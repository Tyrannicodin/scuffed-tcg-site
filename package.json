    "@reduxjs/toolkit": "^2.0.1",
    "@types/gapi": "^0.0.47",
    "@types/react-timeago": "^4.1.7",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "googleapi": "^1.0.2",
    "googleapis": "^130.0.0",
    "pg": "^8.11.3",
    "prettier": "^3.1.1",
